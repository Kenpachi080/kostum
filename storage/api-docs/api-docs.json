        "/api/auth/register": {
            "post": {
                "tags": [
                    "Авторизация без api_token"
                ],
                "summary": "Регистрация",
                "description": "Регистрация",
                "operationId": "authRegister",
                "requestBody": {
                    "description": "Регистрация",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "phone, password, email"
                                ],
                                "properties": {
                                    "phone": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "+7708"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "123"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "testemail@mail.ru"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Возврощается полная информация про пользователя, и его токен для дальнейшей работы с юзером",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "user": {
                                            "type": "object",
                                            "example": []
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "18|TuQoXj84z5IxclUeRK89bSS4839sQfJ8KsQRVRVO"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
